    \entry{doi:10.2514/1.I010037}{article}{}{}
      \name{author}{1}{}{%
        {{un=0,uniquepart=base,hash=6d1240f32cfc61ae83505e57ae7481c6}{%
           family={Nguyen},
           familyi={N\bibinitperiod},
           given={Nhan},
