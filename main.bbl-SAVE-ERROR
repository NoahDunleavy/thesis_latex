      \strng{namehash}{1deb4a8711ae24c0a7ebab7ad844801b}
      \strng{fullhash}{1deb4a8711ae24c0a7ebab7ad844801b}
      \strng{fullhashraw}{1deb4a8711ae24c0a7ebab7ad844801b}
      \strng{bibnamehash}{1deb4a8711ae24c0a7ebab7ad844801b}
      \strng{authorbibnamehash}{1deb4a8711ae24c0a7ebab7ad844801b}
      \strng{authornamehash}{1deb4a8711ae24c0a7ebab7ad844801b}
      \strng{authorfullhash}{1deb4a8711ae24c0a7ebab7ad844801b}
